<style type="text/css">
    input.rfq_text_input {
        width: 250px;
    }

    div.box {
        display: none;
    }

    div.ts8_it {
        background: #DEF2E1;
        padding: 15px 10px 60px 30px;
        margin: 25px;
    }

    div.ts8_ie {
        background: #E5E2AE;
        padding: 15px 10px 60px 30px;
        margin: 25px;
    }

