<script type="text/javascript">
    var enclosureAccessories = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model.enclosureAccessories));
    var enclosureString = '@Model.enclosures'.replaceAll('&quot;', '"');
    var enclosures = JSON.parse(enclosureString);
    var category = '@Model.product_category';
    var industry = '@Model.product_industry';

    $(document).ready(function () {
        var firstTime = true;
        changeCompany(false);
        industrySection(industry.toLowerCase());

        $(function () {
            $('#updated_quote').ready(function () {
                quoteCheck($('#updated_quote').val());
            });
            $('#updated_quote').change(function () {
                quoteCheck($(this).val());
            });
        });

        function quoteCheck(val) {
            $('.quote_num').hide();
            $('.quote-text').remove();
            $('.product-wrapper').show();
            $('.submit-btn').addClass('hide');
            $('.next-button').removeClass('hide');
            $('.enclosure-tab,.product-information-tab').removeClass('dont-show');
            $('.quote_num .notes-help-text').addClass('hide');
            if (val == '')
                return;
            if (val == 'Re-validate existing quote, no configuration changes' || val == 'Change configuration of existing specially quoted product') {
                $('.quote_num').show();
                $('.enclosure-tab,.product-information-tab').addClass('dont-show');
                if (val == 'Change configuration of existing specially quoted product') {
                    $('.quote_num .field-validation-valid').after('<div class="help-text quote-text">A new quote number may be assigned</div>');
                    $('.quote_num .notes-help-text').removeClass('hide');
                }
            }
        }
    });

    $(document).on('change', '.door-dropdown', function (e) {
        var obj = $(this);
        doorDropdownChange(obj);
    });

    function doorDropdownChange(obj) {
        var parent = obj.parents('.tab.enclosure-section-tab');
        var wrapper = $('.selection-wrapper', parent);
        if (obj.val() == '' || obj.val() == 'Without' || obj.val().indexOf("Rear wall") > -1)
            wrapper.addClass('hide');
        else
            wrapper.removeClass('hide');
    }

    $(document).on('change', 'select[name="sold_to_party"]', function (e) {
        changeCompany(true);
    });

    function changeCompany(triggered) {
        var select = $('select[name="sold_to_party"]');
        $('select[name="location"] option:not(.first)').addClass('hide');
        if (triggered)
            $('select[name="location"],input[name="sap_account_num"]').val('');
        var comp = select.length > 0 ? select.val() : '';
        if (typeof comp === 'undefined' || comp == '')
            return;
        var compId = $('option:selected', select).attr('data-id');
        $('select[name="location"] option[data-company="' + compId + '"]').removeClass('hide');
    }

    $(document).on('change', 'select[name="location"]', function (e) {
        $('input[name="sap_account_num"]').val('');
        if ($(this).val() == '')
            return;
        $('input[name="sap_account_num"]').val($('option:selected', this).attr('data-sap'));
    });

    $(document).on('change', '.trigger-filter', function (e) {
        var parent = $(this).parents('.tab.enclosure-section-tab');
        if ($('.special-dimensions', parent).is(':checked'))
            return false;
        $('.btn-apply-filters', parent).trigger('click');
    });

    $(document).on('click', '[name="product_industry"]', function (e) {
        var val = $(this).val().toLowerCase();
        industrySection(val);
    });

    function industrySection(val) {
        $('.' + val + '-selection:not(.double-door)').removeClass('hide');
        $('.' + (val == 'industrial' ? 'it' : 'industrial') + '-selection').addClass('hide');
        if (val == 'it')
            $('.it-hide:not(.dont-show)').addClass('dont-show');
        else
            $('.it-hide').removeClass('dont-show');
    }

    var rackUnits = { '47': 2200, '48': 2200, '42': 2000, '24': 1200 };
    $(document).on('click', '.btn-apply-filters', function (e, triggered) {
        triggered = triggered || false;
        e.preventDefault();
        var obj = $(this);
        var parent = obj.parents('.tab.enclosure-section-tab');
        if (populateFilters) {
            $('[name]:not(.disabled):not([type="radio"])', parent).val('');
        }
        var tab = parent.parent('.tab-pane');
        if (tab.attr('data-part') != '') {
            var data = { part_number: tab.attr('data-part') };
        }
        else {
            var data = $('[name]', parent).serializeArray();
            data.push({ name: 'product_type', value: category });
            data.push({ name: 'industry', value: (category == 'Freestanding' ? industry : "") });
            data.push({ name: 'return_filters', value: true });
        }
        $.ajax({
            url: '/RFQNewTool/GetProducts',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                obj.html('Loading....');
            },
            success: function (res) {
                createProductsTable(res.products, parent, tab.attr('data-part'));
                var materials = [];
                $.each(['material', 'family', 'rating', 'enclosure_type'], function (i, field) {
                    if (!res.hasOwnProperty(field) || res[field].length == 0)
                        return;
                    var inputVal = $('select[name="' + field + '"]').val();
                    if (inputVal == '') {
                        var html = '<option value="">Select Option</option>';
                        $.each(res[field], function (j, item) {
                            var optVal = item;
                            if (field == 'material') {
                                if (item.indexOf('-') > -1) {
                                    optVal = item.split('-')[0].trim();
                                    if (materials.indexOf(optVal) > -1) {
                                        return;
                                    }
                                }
                                materials.push(optVal);
                            }
                            html += '<option value="' + optVal + '">' + optVal + '</option>';
                        });
                        $('select[name="' + field + '"]', parent).html(html);
                    }
                });
                populateFilters = false;
                prefillSelections(tab.attr('data-index'));
            },
            complete: function () {
                obj.html('Apply Filters');
            }
        });
        return false;
    });

    $(document).on('click', '.btn-apply-accessory-filters', function (e) {
        e.preventDefault();
        var obj = $(this);
        var parent = obj.parents('.tab.enclosure-section-tab');
        var wrapper = obj.parents('.accessory-filters');
        var data = [];
        $('[data-name]', wrapper).each(function () {
            data.push({ name: $(this).attr('data-name'), value: $(this).val() });
        });
        data.push({ name: 'product_type', value: category });
        var enclosure = parent.parents('.tab-pane');
        data.push({ name: 'product_family', value: $('.tab.first-enclosure-section table input[type="radio"]:checked', enclosure).attr('data-family') });
        $.ajax({
            url: '/RFQNewTool/GetAccessories',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                obj.html('Loading....');
            },
            success: function (res) {
                var panel = $('.accessory-panel .panel-collapse.in:visible', parent);
                if (!res.accessories || res['accessories'].length == 0)
                    return;
                var table = $('.accessory-table', panel);
                var html = createAccessoriesTable(res.accessories, table.attr('data-name'));
                $('tbody', table).html(html);
                table.removeClass('hide');
            },
            complete: function () {
                obj.html('Apply Filters');
            }
        });
        return false;
    });

    function createAccessoriesTable(products, inputName) {
        var html = '';
        if (products.length > 0) {
            $.each(products, function (i, prod) {
                html += '<tr><td class="text-center"><input type="radio" value="' + prod.part_number + '" name="' + inputName + '" class="no-trigger accessory-radio" /><td>' + prod.part_number + '</td><td>' + prod.product_type + '</td><td>' + prod.family + '</td><td>' + prod.group_1 + '</td><td>' + prod.group_2 + '</td><td>' + prod.description_1 + '</td><td>' + prod.description_2 + '</td><td>' + prod.description_3 + '</td><td>' + (prod.color == 'null' ? '' : prod.color) + '</td><td>' + prod.material + '</td></tr> ';
            });
        }
        else {
            html = '<tr><td colspan="11" class="text-center">No accessories found</td></tr>';
        }
        return html;
    }

    $(document).on('click', '.accessory-radio', function (e) {
        var panel = $(this).parents('.accessory-panel');
        $('.hdn-accessory-input,.selected-accessory-wrapper', panel).remove();
    });

    $(document).on('change', 'select[data-name="group_1"]', function (e) {
        var wrapper = $(this).parents('.accessory-filters');
        var group = $(this).val();
        var select = $('select[data-name="group_2"]', wrapper);
        if (group == '') {
            select.html('<option value="">Select Option</option>').val('');
            return false;
        }
        var data = [];
        data.push({ name: 'group_1', value: group });
        data.push({ name: 'product_type', value: category });
        var enclosure = wrapper.parents('.tab-pane');
        data.push({ name: 'product_family', value: $('.tab.first-enclosure-section table input[type="radio"]:checked', enclosure).attr('data-family') });
        $.ajax({
            url: '/RFQNewTool/GetAccessoryGroups',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                select.addClass('hide');
                select.after('<div class="loader">Loading....</div>');
            },
            success: function (res) {
                if (!res.groups || res['groups'].length == 0)
                    return;
                var options = '<option value="">Select Option</option>';
                $.each(res.groups, function(i, group) {
                    options += '<option value="' + group + '">' + group + '</option>';
                });
                $('select[data-name="group_2"]', wrapper).html(options).val('');
            },
            complete: function () {
                select.removeClass('hide');
                $('.loader').remove();
            }
        });
    });

    $(document).on('click', '.reset-filters', function (e) {
        e.preventDefault();
        populateFilters = true;
        $('.btn-apply-filters').trigger('click',[true]);
        return false;
    });

    $(document).on('change', 'input[name="dimension_type"]', function (e) {
        var obj = $(this);
        var parent = obj.parents('.tab.enclosure-section-tab');
        var dimensionType = $('.dimension-type:checked').val();
        $('input[type="number"]', parent).attr('placeholder', dimensionType);
        if (dimensionType == 'mm')
            $('input[type="number"]', parent).attr('step', 100);
        else
            $('input[type="number"]', parent).attr('step', 1);
    });

    function createProductsTable(products, parent, selectedPart) {
        var html = '';
        $('.products-table .material-num').removeClass('hide');
        if (products.length > 0) {
            $.each(products, function (i, prod) {
                var materialParts = prod.material.split(" - ");
                var prodHeight = prod.height;
                if (prod.product_type.toLowerCase() == "freestanding" && prod.industry == "IT") {
                    prodHeight = rackUnits[prodHeight];
                }
                html += '<tr><td class="text-center"><input type="radio" value="' + prod.part_number + '" name="enclosure[' + parent.parent('.tab-pane.active').attr('data-index') + '][part_number]" data-color="' + prod.color + '" data-family="' + prod.family + '" data-height="' + prod.height + '" data-depth="' + prod.depth + '"  data-doors="' + prod.door + '" class="no-trigger" ' + (prod.part_number == selectedPart ? 'checked' : '') + ' /><td>' + prod.product_type + '</td><td>' + prod.industry + '</td><td>' + (prod.family == 'null' ? '' : prod.family) + '</td><td>' + prod.part_number + '</td><td class="material-num">' + (materialParts.length > 1 ? materialParts[1].trim() : "") + '</td><td>' + prod.enclosure_type + '</td><td>' + (materialParts[0] == 'null' ? '' : materialParts[0]) + '</td><td>' + (prod.color == 'null' ? '' : prod.color) + '</td><td>' + prodHeight + (prod.product_type.toLowerCase() == "freestanding" && prod.industry == "IT" ? " (" + prod.height + "U)" : "") + ' x ' + prod.width + ' x ' + prod.depth + '</td><td>' + (Math.floor(prodHeight / 25.4)) + ' x ' + (Math.floor(prod.width / 25.4)) + ' x ' + (Math.floor(prod.depth / 25.4)) + ' </td><td class="text-center">' + prod.rating + '</td><td class="text-center">' + prod.door + '</td><td>' + prod.additional_information + '</td></tr > ';
            });
        }
        else {
            html = '<td colspan="13" class="text-center">No products found</td>';
        }
        $('.products-table tbody', parent).html(html);
        if (category == 'Freestanding')
            $('.products-table .material-num').addClass('hide');
        $('.products-table', parent).removeClass('hide');
        if (selectedPart != '') {
            $('.products-table input[type="radio"][value="' + selectedPart + '"]').trigger('click');
        }
        
    }

    $(document).on('click', '.products-table input[type="radio"]', function (e) {
        var obj = $(this);
        var row = obj.parents('tr');
        //$('.door-handle-wrapper').removeClass('hide');
        //if ($('td:nth-child(2)', row).text() == 'Freestanding' && $('td:nth-child(4)', row).text() == 'TS8' && $('td:nth-child(8)', row).text() == '316 Stainless Steel')
        //    $('.door-handle-wrapper').addClass('hide');
        var enclosure = obj.parents('.tab-pane');
        if (enclosure.attr('data-index') == 0) {
            var enclosureHeight = obj.attr('data-height');
            var enclosureDepth = obj.attr('data-depth');
            var num = $('select[name="suited_enclosures_ie"]').val();
            if (num != '') {
                for (var i = 2; i <= num; i++) {
                    var enclosureDiv = $('.tab-pane#enclosure_' + i);
                    $('input[name="height"],select[name="height_IT"]', enclosureDiv).val(enclosureHeight).addClass('disabled').attr('readonly', true);
                    $('input[name="depth"],select[name="depth_IT"]', enclosureDiv).val(enclosureDepth).addClass('disabled').attr('readonly', true);
                }
            }
        }
        var wrapper = $('.accessory-filters',enclosure);
        var select = $('select[data-name="group_1"]', wrapper);
        var data = [];
        data.push({ name: 'product_type', value: category });
        data.push({ name: 'product_family', value: obj.attr('data-family') });
        $.ajax({
            url: '/RFQNewTool/GetAccessoryGroups',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                select.addClass('hide');
                select.after('<div class="loader">Loading....</div>');
            },
            success: function (res) {
                if (!res.groups || res['groups'].length == 0)
                    return;
                var options = '<option value="">Select Option</option>';
                $.each(res.groups, function (i, group) {
                    options += '<option value="' + group + '">' + group + '</option>';
                });
                select.html(options).val('');
            },
            complete: function () {
                select.removeClass('hide');
                $('.loader').remove();
            }
        });
        var doors = obj.attr('data-doors');
        if (category == "Freestanding") {
            $('.' + industry.toLowerCase() + '-selection.' + (doors == '1' ? 'single' : 'double') + '-door', enclosure).removeClass('hide');
            $('.' + industry.toLowerCase() + '-selection.' + (doors == '1' ? 'double' : 'single') + '-door', enclosure).addClass('hide');
        }
        else {
            $('.wallmount-show .' + (doors == '1' ? 'single' : 'double') + '-door', enclosure).removeClass('hide');
            $('.wallmount-show .' + (doors == '1' ? 'double' : 'single') + '-door', enclosure).addClass('hide');
        }
    });

    $(document).on('click', '.btn-apply-fan-filters', function (e) {
        e.preventDefault();
        var obj = $(this);
        var parent = obj.parents('.panel-body');
        var data = $('[name]', parent).serializeArray();
        var part = parent.attr('data-part');
        data.push({ name: 'part_number', value: part });
        $.ajax({
            url: '/RFQNewTool/GetFans',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                obj.html('Loading....');
            },
            success: function (res) {
                createFansTable(res.products, parent.parents('.tab.enclosure-section-tab'), part);
            },
            complete: function () {
                obj.html('Apply Filters');
            }
        });
        return false;
    });

    function createFansTable(products, parent, part) {
        var html = '';
        if (products.length > 0) {
            $.each(products, function (i, prod) {
                html += '<tr><td class="text-center"><input type="radio" value="' + prod.part_number + '" name="enclosure[' + parent.parent('.tab-pane.active').attr('data-index') + '][fan_part_number]" class="no-trigger fan-select" /><td>' + prod.part_number + '</td><td>' + prod.description + '</td><td>' + prod.cfm + '</td><td>' + prod.voltage + '</td><td>' + prod.color + '</td><td>' + prod.additional_description + '</td></tr > ';
            });
        }
        else {
            html = '<td colspan="7" class="text-center">No Fans found</td>';
        }
        $('.fans-table tbody', parent).html(html);
        $('.fans-table', parent).removeClass('hide');
        if (part !== undefined && part != null && part != '')
            $('.fan-select[value="' + part + '"]', parent).trigger('click');
    }

    $(document).on('click', '.btn-apply-fanfilters-filters', function (e) {
        e.preventDefault();
        var obj = $(this);
        var parent = obj.parents('.panel-body');
        var data = $('[name]', parent).serializeArray();
        var part = parent.attr('data-part');
        data.push({ name: 'part_number', value: part });
        $.ajax({
            url: '/RFQNewTool/GetFanFilters',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                obj.html('Loading....');
            },
            success: function (res) {
                createFanFiltersTable(res.products, parent.parents('.tab.enclosure-section-tab'), part);
            },
            complete: function () {
                obj.html('Apply Filters');
            }
        });
        return false;
    });

    function createFanFiltersTable(products, parent, part) {
        var html = '';
        if (products.length > 0) {
            $.each(products, function (i, prod) {
                html += '<tr><td class="text-center"><input type="radio" value="' + prod.part_number + '" name="enclosure[' + parent.parent('.tab-pane.active').attr('data-index') + '][fan_filter_part_number]" class="no-trigger fanfilter-select" /><td>' + prod.part_number + '</td><td>' + prod.description + '</td><td>' + prod.color + '</td></tr >';
            });
        }
        else {
            html = '<td colspan="4" class="text-center">No Fan Filters found</td>';
        }
        $('.fanfilters-table tbody', parent).html(html);
        $('.fanfilters-table', parent).removeClass('hide');
        if (part !== undefined && part != null && part != '')
            $('.fanfilter-select[value="' + part + '"]', parent).trigger('click');
    }

    $(document).on('click', '.fan-select', function (e) {
        $('.fan-panel').removeClass('hide');
        $('.fan-product-number').text($(this).val());
        $('a[href="#FanTwo"]').trigger('click');
    });

    $(document).on('click', '.fanfilter-select', function (e) {
        $('.filter-panel').removeClass('hide');
        $('a[href="#fanFilterTwo"]').trigger('click');
    });

    $(document).on('click', '.skip-fan-filters', function (e) {
