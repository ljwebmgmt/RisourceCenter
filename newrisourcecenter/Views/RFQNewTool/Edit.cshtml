    $(document).on('click', 'input[name="default_baying_accessories"]', function (e) {
        $('.baying_hardware_wrapper').addClass('hide');
        if ($(this).val() == "0")
            $('.baying_hardware_wrapper').removeClass('hide');
    });

    $(document).on('click', 'input.accessory-guided-chk', function (e) {
        var obj = $(this);
        parent = obj.parents('.enclosure-tab');
        if (obj.val() == 1) {
            $('.guided-accessories', parent).removeClass('hide');
            $('.manual-accessories', parent).addClass('hide');
        }
        else {
            $('.guided-accessories', parent).addClass('hide');
            $('.manual-accessories', parent).removeClass('hide');
        }
    });

    $(document).on('blur', '.accessory-input', function (e) {
        var obj = $(this);
        var row = obj.parents('tr');
        var part = obj.val();
        $('td:nth-child(3)', row).text('');
        if (part == '')
            return;
        if (enclosureAccessories.hasOwnProperty(part))
            $('td:nth-child(3)', row).text(enclosureAccessories[part]);
    });

    $(document).on('click', '.btn-add-another-accessory-row', function (e) {
        e.preventDefault();
        var obj = $(this);
        var tab = obj.parents('.tab.enclosure-section-tab');
        if (!validateTab(obj.parents('.panel-body')))
            return false;
        var row = $('.manual-accessories table tbody tr:nth-child(1)', tab).clone();
        var index = $('.manual-accessories table tbody tr', tab).length;
        $('[name]', row).each(function () {
            var name = $(this).attr('name');
            $(this).attr('name', name.replace('[manual_accessory][0]', '[manual_accessory][' + index + ']'));
            $(this).val('');
        });
        $('td:nth-child(3)', row).text('');
        $('.btn-remove-accessory-row', row).removeClass('hide');
        $('.manual-accessories table tbody', tab).append(row);
    });

    $(document).on('click', '.btn-remove-accessory-row', function (e) {
        $(this).parents('tr').remove();
    });

    function enclosuresInput() {
        var rfqVariant = parseInt($('input[name="variant"]').val());
        var tab = $('.first-edit-section').parent('.tab-pane');
        var index = tab.attr('data-index');
        var namePart = 'enclosure[' + index + ']';
        $('.enclosure-section-tab:not(.no-edit)', tab).each(function () {
            $('[name]', this).each(function () {
                if ($(this).parents('.form-group.hide').length > 0)
                    return;
                if (($(this).is(':radio') || $(this).is(':checkbox')) && !$(this).is(':checked'))
                    return;
                var key = $(this).attr('name').replace(namePart, '');
                key = key.replace('[', '');
                key = key.replace(']', '');
                enclosures[index][key] = $(this).val();
            });
        });
        var variants = [];
        $('[data-name="' + namePart + '[variants]"]:checked,input[type="hidden"][data-name="' + namePart + '[variants]"]').each(function () {
            var val = parseInt($(this).val());
            if (val > rfqVariant)
                rfqVariant = val;
            var variant = $(this).attr('data-value');
            if ($.inArray(variant, variants) !== -1) {
                variants.push(variant);
            }
            
        });
        enclosures[index]['variants'] = variants.join(', ');
        $('input[name="variant"]').val(rfqVariant);
    }

    $(document).on('click', '.btn-save-enclosures', function (e) {
        e.preventDefault();
        var obj = $(this);
        enclosuresInput();
        $.ajax({
            url: '/RFQNewTool/SaveEnclosures',
            type: 'POST',
            data: { rfqID: '@Model.ID', enclosures: JSON.stringify(enclosures), variant: $('input[name="variant"]').val() },
            dataType: 'json',
            beforeSend: function () {
                obj.html('Loading....');
            },
            success: function (res) {
                window.location.reload();
            },
            complete: function () {
                obj.html('Save RFQ');
            }
        });
        return false;
    });
</script>
<script type="text/javascript">
    var type = category == "Wallmounted" ? "_wallmount" : (industry == "IT" ? "_it" : "");

    var field_sections = {
        'enclosure': ['part_number', 'special_dimensions', 'width', 'height', 'depth'],
        'paint': ['special_paint']
    };
    field_sections['front' + (category == "Wallmounted" ? '-wallmount' : '')] = ['front_door' + type];
    field_sections['rear'] = ['rear_door' + type];
    field_sections['mounting' + (category == "Wallmounted" ? '-wallmount' : '')] = ['mounting_panel' + (category == "Wallmounted" ? "_wallmount" : "")];
    field_sections = {
        ...field_sections, ...{
            'roof': ['roof' + (industry == "IT" ? "_it" : "")],
            'base': ['base' + (industry == "IT" ? "_it" : "")],
            'sidewall': ['sidewalls'],
            'climate': ['climate_type'],
            'partition': ['partition_walls'],
            'plinth': ['plinths'],
            'seismic': ['seismic'],
            'accessory': ['accessories'],
            'modification': ['modifications']
        }
    };
    var sectionTitles = {
        'enclosure': 'Enclosure',
        'paint': 'Special Paint',
        'front': 'Enclosure Front',
        'front-wallmount': 'Enclosure Front',
        'rear': 'Enclosure Rear',
        'roof': 'Roof',
        'base': 'Base',
        'sidewall': 'Sidewalls',
        'climate': 'Climate',
        'partitiom': 'Partition Walls',
        'plinth': 'Plinths',
        'seismic': 'Seismic',
        'accessory': 'Accessories',
        'modification': 'Modifications',
        'mounting': 'Mounting Panel',
        'mounting-wallmount': 'Mounting Panel'
    };
    //var fields = [
    //    'part_number','special_dimensions','width','height','depth','special_paint','front_door' + type,'rear_door' + type,'mounting_panel' + (category == "Wallmounted" ? "_wallmount" : ""),'roof' + (industry == "IT" ? "_it" : ""),
    //    'base' + (industry == "IT" ? "_it" : ""),'sidewalls','climate_type','partition_walls','plinths','seismic','accessories','modifications'
    //];
    var conditional_fields = {
        'special_paint': ['paint_color'],
        'front_door': ['front_door_hinge','front_door_handle','front_door_insert','front_door_print_pocket','front_door_stop'],
        'front_door_it': ['front_door_hinge','front_door_handle_it','front_door_insert_it','front_door_print_pocket','front_door_stop'],
        'front_door_wallmount': ['front_door_wallmount_hinge','front_door_wallmount_handle'],
        'rear_door': ['rear_door_hinge','rear_door_handle','rear_door_insert','rear_door_print_pocket','rear_door_stop'],
        'rear_door_it': ['rear_doorhinge','rear_door_handle_it','rear_door_insert_it','rear_door_print_pocket','rear_door_stop'],
        'rear_door_wallmount': ['rear_door_wallmount_hinge','rear_door_wallmount_handle'],
        'sidewalls': ['sidewall_installed'],
        'climate_type': ['ac_part_number','ac_surface','ac_location','ac_quantity','heat_part_number','heat_surface','heat_location','heat_quantity','fan_part_number','fan_installed','fan_surface','fan_location','fan_quantity','fan_filter_part_number','fan_filter_installed','fan_filter_surface','fan_filter_location','fan_filter_quantity'],
        'plinths': ['plinth_height','plinth_side_mounting_panel'],
        'seismic': ['seismic_with_plinth','seismic_with_mpl'],
        'accessories': ['guided_accessories'],
        'modifications': ['variants','modification_front_door','modification_rear_door','modification_left_sidewall','modification_right_sidewall','modification_mounting_panel','modification_roof','modification_plinth','modification_gland_plates','modification_wallmount_body','modification_wallmount_door','modification_wallmount_mounting_panel','modification_wallmount_gland_plates']
    };
    $(document).ready(function () {
        var notEditable = ['enclosure'];
        $.each(enclosures,function(i,encl) {
            var tab = $('.view-form .tab-pane[data-index="' + i + '"]');
            var html = '';
            $.each(field_sections, function (section, fields) {
                var sectionHtml = '';
                $.each(fields, function (j, field) {
                    var fieldHtml = getFieldHtml(encl, field);
                    if (fieldHtml != '')
                        sectionHtml += fieldHtml;
                    if (conditional_fields.hasOwnProperty(field)) {
                        $.each(conditional_fields[field], function (k, childField) {
                            var fieldHtml = getFieldHtml(encl, childField);
                            if (fieldHtml != '')
                                sectionHtml += fieldHtml;
                        });
                    }
                    if (field == 'accessories') {
                        var key = (encl['guided_accessories'] == 0 ? 'manual_' : '') + 'accessory';
                        for (var l = 0; l < 100; l++) {
                            var accessoryKey = key + '[' + l + ']';
                            if (!encl.hasOwnProperty(accessoryKey + '[part_number]'))
                                break;
                            $.each(['part_number', 'quantity', 'install_location'], function (m, accessoryField) {
                                var fieldHtml = getFieldHtml(encl, accessoryKey + '[' + accessoryField + ']', 'Accessory # ' + (l + 1) + ' ' + accessoryField.replace('_', ' '));
                                if (fieldHtml != '')
                                    sectionHtml += fieldHtml;
                            });
                        }
                    }
                });
                if (sectionHtml != '')
                    html += '<div class="section-header">' + sectionTitles[section] + ' Section' + ($.inArray(section, notEditable) == -1 ? '&nbsp;&nbsp;<button type="button" class="btn btn-link edit-section" data-section="' + section + '" data-enclosure="' + (i + 1) + '">[Edit]</button>' : '') + '</div>' + sectionHtml;
            });
            tab.html(html);
        });
    });

    function getFieldHtml(encl, field, label) {
        var label = label || '';
        var val = encl.hasOwnProperty(field) ? encl[field] : '';
        if (val == null || val == '')
            return '';
        if (val == '1')
            val = "Yes";
        else if (val == "0")
            val = "No";
        else if (val.toLowerCase() == "other" && encl.hasOwnProperty(field + '[other]'))
            val = encl[field + '[other]'];
        return '<div class="form-group" style="margin-bottom:0;"><label class="control-label col-md-2 text-capitalize">' + (label == '' ? field.replaceAll('_', ' ') : label) + '</label><div class="col-md-10">' + val + '</div></div>';
    }

    $(document).on('click', '.edit-section', function (e) {
        e.preventDefault();
        var obj = $(this);
        var form = $('.edit-form');
        form.removeClass('hide');
        $('.nav-tabs li a[href="#enclosure_' + obj.attr('data-enclosure') + '"]').trigger('click');
        $('.tab-pane .tab', form).addClass('hide');
        $('.first-edit-section').removeClass('first-edit-section');
        var tab = $('.tab-pane .tab.' + obj.attr('data-section') + '-section', form);
        tab.removeClass('hide').addClass('first-edit-section enclosure-active');
        $('html,body').animate({ scrollTop: form.offset().top });
        $('select[name]:visible', tab).trigger('change');
        $('input[name]:checked:visible', tab).trigger('click');
        if (tab.hasClass('modification-section'))
            triggerModifications(tab);                                         
        return false;
    });

    $(document).on('click', '.btn-reset-climate-filters', function (e) {
        e.preventDefault();
        var obj = $(this);
        var panel = obj.parents('.panel-body');
        $('select,input[type="text"]', panel).val('');
        $('input[type="checkbox"],input[type="radio"]', panel).prop('checked', false);
        $('.apply-filters-btn', panel).trigger('click');
        return false;
    });
</script>
