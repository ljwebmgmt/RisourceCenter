    $(document).on('click', 'input.special-dimensions', function (e) {
        var obj = $(this);
        var parent = obj.parents('.enclosure-tab');
        if (obj.is(':checked') == 1) {
            $('.special-required', parent).attr('required', true);
            $('.products-table,.top-buttons-wrapper', parent).addClass('hide');
            $('input.enclosure-width', parent).val($('input[name="width"]', parent).val());
            $('input.enclosure-height', parent).val($('input[name="height"]', parent).val());
            $('input.enclosure-depth', parent).val($('input[name="depth"]', parent).val());
            var input = $('.variant-4', parent);
