<script type="text/javascript">
    $(document).ready(function () {
        productCategory(true);
        createEnclosures();
        setTimeout(function () {
            $('[name="default_baying_accessories"]:checked').trigger('click');
            fillEnclosures();
        }, 500);


    });

    function fillEnclosures() {
        $.each(enclosures, function (i, encl) {
            var tab = $('.tab-pane[data-index="' + i + '"]');
            tab.attr('data-part', encl.part_number);
            $('.first-enclosure-section .btn-apply-filters', tab).trigger('click');
        });
    }

    function prefillSelections(i) {
        if ((enclosures.length - 1) < i)
            return;
        var tab = $('.tab-pane[data-index="' + i + '"]');
        var encl = enclosures[i];
        var namePart = 'enclosure[' + i + ']';
        for (const [key, value] of Object.entries(encl)) {
            if (key != 'part_number') {
                var input = $('.form-group:not(.hide) [name="' + namePart + '[' + key + ']"]', tab);
                if (input.length > 0) {
                    if (input.length > 1) {
                        input.each(function (j, inputObj) {
                            if ($(inputObj).val() == value)
                                $(inputObj).attr('checked', true).trigger('change').trigger('click');
                        });
                    }
                    else {
                        input.val(value);
                        if (input.hasClass('door-dropdown'))
                            doorDropdownChange(input);
                        else
                            input.trigger('change');
                    }
                }
                if (key == 'climate_type')
                    climateEdit(encl, i);
                else if (key == 'accessories')
                    accessoriesEdit(encl, i);
            }
        }
    }

    function validateTab(tab) {
        var bValid = true;
        $('[data-val-required],[required]', tab).each(function () {
            if ($(this).is(":visible") && $(this).val() == '') {
                bValid = false;
                return false;
            }
        });
        if (bValid && tab.hasClass('validate-radio') && $('input[name][type="radio"]:checked', tab).length == 0) {
            bValid = false;
        }
        $('.required-radio:visible', tab).each(function () {
            if ($('input[type="radio"][name]:checked', this).length == 0) {
                bValid = false;
                return;
            }
        });
        if (!bValid) {
            alert('Please fill all required fields');
        }
        return bValid;
    }
    var populateFilters = false;

    function triggerModifications(tab) {
        var parent = tab.parents('.enclosure-tab');
        var maxVariant = 0;
        $('.variant:checked', tab).each(function () {
            if ($(this).val() > maxVariant) {
                maxVariant = $(this).val();
            }
        });
        $('.modifications-wrapper .wrapper', tab).addClass('hide');
        if (maxVariant > 1) {
            $('.modifications-wrapper .wrapper[data-type="' + category.toLowerCase() + '"]', tab).removeClass('hide');
        }
    }

    $(document).on('click', 'input[type="checkbox"].variant', function (e) {
        triggerModifications($(this).parents('.tab.enclosure-section-tab'));
    });

    $(document).on('click', '.next-enclosure-button', function (e) {
        e.preventDefault();
        var enclosure = $('.tab-pane.active');
        var tab = $('.tab.enclosure-active', enclosure);
        if (!validateTab(tab))
            return false;
        var next = tab.nextAll('.tab.hide:not(.dont-show)').first();
        next.removeClass('hide').addClass('enclosure-active');
        tab.removeClass('enclosure-active').addClass('hide');
        if (!$('.prev-button').hasClass('prev-enclosure-button'))
            $('.prev-button').addClass('prev-enclosure-button');
        $('.prev-enclosure-button').removeClass('hide')
        if (next.hasClass('last-enclosure-section')) {
            $(this).addClass('hide');
        }
        if ($('.product-summary-wrapper', next).length > 0)
            showEnclosureSummary(enclosure, next);
        $('select[name]:visible', next).trigger('change');
        $('input[name]:checked:visible', next).trigger('click');
        return false;
    });

    $(document).on('click', '.prev-enclosure-button', function (e) {
        e.preventDefault();
        var enclosure = $('.tab-pane.active');
        var tab = $('.tab.enclosure-active', enclosure);
        var prev = tab.prevAll('.tab.hide:not(.dont-show)').first();
        prev.removeClass('hide').addClass('enclosure-active');
        tab.removeClass('enclosure-active').addClass('hide');
        $('.next-enclosure-button').removeClass('hide');
        if (prev.hasClass('first-edit-section'))
            $(this).addClass('hide');
        if ($('.product-summary-wrapper', prev).length > 0)
            showEnclosureSummary(enclosure, prev);
        return false;
    });

    function showEnclosureSummary(enclosure, tab) {
        var tabIndex = tab.index();
        var html = '<div class="card-panel z-depth-2 summaryPanel">';
        if ($('.special-dimensions:checked', enclosure).length == 0) {
            var chkPart = $('.tab.first-enclosure-section table input[type="radio"]:checked', enclosure);
            var partRow = chkPart.parents('tr');
            html += '<p class="centerBOMList">Part Number: <span class="rittalPink-text">' + chkPart.val() + ' - ' + $('td:nth-child(7)', partRow).text() + ' - ' + $('td:nth-child(10)', partRow).text() + '</span></p>';
        }
        else {
            html += '<p class="centerBOMList">Special Size (HxWxD): <span class="rittalPink-text">' + $('.enclosure-height', enclosure).val() + ' x ' + $('.enclosure-width', enclosure).val() + ' x ' + $('.enclosure-depth', enclosure).val() + '</span></p>';
            html += '<p class="centerBOMList">Material: <span class="rittalPink-text">' + $('.enclosure-material', enclosure).val() + '</span></p>';
            html += '<p class="centerBOMList">UL/Nema Rating: <span class="rittalPink-text">' + $('.enclosure-rating', enclosure).val() + '</span></p>';
        }

        $('.tab:not(.first-enclosure-section):not(.dont-show)', enclosure).each(function () {
            var enclosureTab = $(this);
            if (enclosureTab.index() >= tabIndex)
                return;
            var tabHtml = '';
            if (!$(this).hasClass('accessory-tab')) {
                $('.form-group', enclosureTab).each(function () {
                    var val = '';
                    if ($('.other-input', this).length > 0 && $('.other-input', this).val() != '')
                        val = $('.other-input', this).val();
                    else if ($('select[name]', this).length > 0)
                        val = $('select[name]', this).val();
                    else if ($('input[type="radio"][name]:checked', this).length > 0)
                        val = $('input[type="radio"][name]:checked', this).attr('data-value');
                    else if ($('input[type="text"][name]', this).length > 0)
                        val = $('input[type="text"][name]', this).val();
                    if (typeof val === 'undefined' || val == null || val == '') {
                        if ($('.paint-color', this).length > 0) {
                            tabHtml += '<p class="centerBOMList">Default Paint Color: <span class="rittalPink-text">' + chkPart.attr('data-color') + '</span></p>';
                            return;
                        }
                        else
                            return;
                    }
                    var label = $('label.col-md-3.control-label', this);
                    tabHtml += '<p class="centerBOMList">' + (typeof label.attr('data-text') !== 'undefined' && label.attr('data-text') != '' ? label.attr('data-text') : label.text()) + ' <span class="rittalPink-text">' + val + '</span></p>';
                });
            }
            else {
                var i = 1;
                if (!$('.selection-wrapper', enclosureTab).hasClass('hide')) {
                    if (!$('.manual-accessories', enclosureTab).hasClass('hide')) {
                        $('.manual-accessories table tbody tr', enclosureTab).each(function () {
                            tabHtml += '<p class="centerBOMList">Accessory # ' + (i++) + ': <span class="rittalPink-text">' + $('td:nth-child(1) input', this).val() + ' - ' + $('td:nth-child(3)', this).text() + ' (Quantity: ' + $('td:nth-child(2) input', this).val() + ')</span></p>';
                        });
                    }
                    else {
                        $('#accessory_accordion .panel', enclosureTab).each(function () {
                            var accName = $('table.accessory-table', this).attr('data-name');
                            var accChk = $('input[name="' + accName + '"]:checked', this);
                            var accRow = accChk.parents('tr');
                            tabHtml += '<p class="centerBOMList">Accessory # ' + (i++) + ': <span class="rittalPink-text">' + accChk.val() + ' - ' + $('td:nth-child(7)', accRow).text() + ' (Quantity: ' + $('input[name="' + accName.replace('part_number', 'quantity') + '"]', this).val() + ')</span></p>';
                        });
                    }
                }
            }
            if (tabHtml != '')
                html += tabHtml;
        });
        html += '</div>';
        $('.product-summary-wrapper', tab).html(html);
    }

    $(document).on('change', 'select[name="suited_enclosures_ie"]', function (e) {
        createEnclosures();
    });

    $(document).on('click', '.btn-add-another-accessory', function (e) {
        e.preventDefault();
        var obj = $(this);
        var tab = obj.parents('.tab.enclosure-section-tab');
        if (!validateTab(obj.parents('.panel-body')))
            return false;
        var accordion = $('#accessory_accordion', tab);
        $('.panel-collapse.in', accordion).removeClass('in');
        var panel = $('.accessory-panel:last', accordion).clone();
        var index = parseInt(accordion.attr('data-accessory-index'));
        $('.panel-collapse', panel).attr('id', 'accessory_' + (index + 1)).addClass('in');
        $('.panel-title a', panel).attr('href', '#accessory_' + (index + 1)).text('Accessory # ' + (index + 1));
        accordion.attr('data-accessory-index', index + 1);
        $('[name]', panel).each(function () {
            var name = $(this).attr('name');
            $(this).attr('name', name.replace('[accessory][' + (index - 1) + ']', '[accessory][' + index + ']'));
            if ($(this).attr('type') == 'radio') {
                $(this).prop('checked', false);
            }
            else {
                $(this).val('');
            }
        });
        var inputName = $('.accessory-table', panel).attr('data-name');
        $('.accessory-table', panel).attr('data-name', inputName.replace('[accessory][' + (index - 1) + ']', '[accessory][' + index + ']'));
        accordion.append(panel);
        $('html,body').animate({ scrollTop: panel.offset().top });
    });

    function createEnclosures() {
        var num = $('select[name="suited_enclosures_ie"]').val();
        if (num == '') {
            for (var j = 2; j <= 5; j++) {
                var tab = $('.tab-pane#enclosure_' + j);
                if (tab.length > 0) {
                    tab.remove();
                    $('.nav-tabs li a[href="#enclosure_' + j + '"]').parent('li').remove();
                }
            }
            return false;
        }
        for (var i = 2; i <= num; i++) {
            if ($('.tab-pane#enclosure_' + i).length > 0)
                return;
            $('.nav-tabs').append('<li role="presentation"><a href="#" data-href="#enclosure_' + i + '" role="tab">Enclosure No. ' + i + '</a></li>');
            var tab = $('.tab-pane#enclosure_1').clone();
            tab.attr('id', 'enclosure_' + i);
            tab.removeClass('active');
            var index = i - 1;
            tab.attr('data-index', index);
            $('[name]', tab).each(function () {
                var name = $(this).attr('name');
                $(this).attr('name', name.replace('[0]', '[' + index + ']'));
            });
            $('.tab-content').append(tab);
        }
    }

    $(document).on('click', 'input[type="radio"][name]:not(.no-trigger)', function (e) {
        var obj = $(this);
        var parent = null;
        if (obj.parents('.enclosure-section-tab').length > 0)
            parent = obj.parents('.tab.enclosure-section-tab');
        else
            parent = obj.parents('.tab.active');
        var wrapper = $('.selection-wrapper', parent);
        if (wrapper.length == 0)
            return;
        if (obj.hasClass('selection-trigger'))
            wrapper.removeClass('hide');
        else
            wrapper.addClass('hide');
    });

    $(document).on('click', 'input[type="radio"][name].val-trigger', function (e) {
        var obj = $(this);
        var parent = null;
        if (obj.parents('.enclosure-section-tab').length > 0)
            parent = obj.parents('.tab.enclosure-section-tab');
        else
            parent = obj.parents('.tab.active');
        $('.selection-wrapper', parent).addClass('hide');
        var wrapper = $('.selection-wrapper[data-type="' + obj.val() + '"]', parent);
        if (wrapper.length == 0)
            return;
        wrapper.removeClass('hide');
    });

    function climateEdit(encl, index) {
        var type = encl.climate_type;
        if (type == null || type == '')
            return;
        var tab = $('.tab-pane[data-index="' + index + '"] .climate-section');
        var wrapper = $('.selection-wrapper[data-type="' + type + '"]', tab);
        if (type == 'AC') {
            $('.ac-panel:first-child .panel-body', wrapper).attr('data-part', encl.ac_part_number);
            $('.btn-apply-ac-filters', wrapper).trigger('click');
        }
        else if (type == 'Fans/Filters') {
            $('.fan-panel:first-child .panel-body'.wrapper).attr('data-part', encl.fan_part_number);
            $('.btn-apply-fan-filters', wrapper).trigger('click');
            $('.filter-panel:first-child .panel-body'.wrapper).attr('data-part', encl.fan_filter_part_number);
            $('.btn-apply-fanfilters-filters', wrapper).trigger('click');
        }
        else if (type == 'Heat Exchangers') {
            $('.heat-panel:first-child .panel-body'.wrapper).attr('data-part', encl.heat_part_number);
            $('.btn-apply-heat-filters', wrapper).trigger('click');
        }
    }

    function accessoriesEdit(encl, index) {
        if (encl.accessories != "1")
            return;
        if (encl.guided_accessories == "1")
            guidedAccessories(encl, index);
        else
            manualAccessories(encl, index);
       
    }

    function manualAccessories(encl, index) {
        var tab = $('.tab-pane[data-index="' + index + '"] .accessory-section');
        var namePart = 'enclosure[' + index + ']';
        for (var i = 0; i < 20; i++) {
            if (!encl.hasOwnProperty('manual_accessory[' + i + '][part_number]') || encl['manual_accessory[' + i + '][part_number]'] == '')
                break;
            var obj = { part: encl['manual_accessory[' + i + '][part_number]'], quantity: encl['manual_accessory[' + i + '][quantity]'], location: encl['manual_accessory[' + i + '][install_location]'] };
            if (i > 0)
                $('.btn-add-another-accessory-row', tab).trigger('click');
            manualAccessoryFill(i, obj, namePart);
        }
    }

    function manualAccessoryFill(i, obj, namePart) {
        setTimeout(function () {
            $('input[name="' + namePart + '[manual_accessory][' + i + '][quantity]"]').val(obj.quantity);
            $('input[name="' + namePart + '[manual_accessory][' + i + '][install_location]"]').val(obj.location);
            var input = $('input[name="' + namePart + '[manual_accessory][' + i + '][part_number]"]');
            input.val(obj.part);
            input.trigger('blur');
        }, 100);
    }

    function guidedAccessories(encl, index) {
        var tab = $('.tab-pane[data-index="' + index + '"] .accessory-section');
        var namePart = 'enclosure[' + index + ']';
        for (var i = 0; i < 20; i++) {
            if (!encl.hasOwnProperty('accessory[' + i + '][part_number]') || encl['accessory[' + i + '][part_number]'] == '')
                break;
            var obj = { part: encl['accessory[' + i + '][part_number]'], quantity: encl['accessory[' + i + '][quantity]'], location: encl['accessory[' + i + '][install_location]'] };
            if (i > 0)
                $('.btn-add-another-accessory', tab).trigger('click');
            guidedAccessoryFill(i, obj, namePart);
        }
    }

    function guidedAccessoryFill(i, obj, namePart) {
        setTimeout(function () {
            $('input[name="' + namePart + '[accessory][' + i + '][quantity]"]').val(obj.quantity);
            $('input[name="' + namePart + '[accessory][' + i + '][install_location]"]').val(obj.location);
            var input = $('input[type="hidden"][name="' + namePart + '[accessory][' + i + '][part_number]"]');
            input.val(obj.part);
            var wrapper = input.parents('.selected-accessory-wrapper');
            $('p', wrapper).html(obj.part);
            wrapper.removeClass('hide');
        }, 100);
    }

    $(document).on('change', 'select[name="product_category"]', function (e) {
        productCategory();
    });

    function productCategory(triggered) {
        var triggered = triggered || false;
        var val = category;
        if (val.toLowerCase().includes('freestanding')) {
            $('.bayed-selection-wrapper').removeClass('hide');
            $('.wallmount-hide.dont-show').removeClass('dont-show');
            $('.wallmount-show').addClass('dont-show');
        }
