@model newrisourcecenter.Models.RFQNewViewModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int comp_type = Convert.ToInt32(Session["companyType"]);
    bool isRittalUser = comp_type == 3 || comp_type == 5;
    Dictionary<long, Dictionary<long, Dictionary<string, string>>> locations = Model.listLocations; ;
    if (!isRittalUser)
    {
        long compId = Convert.ToInt64(Session["companyId"]);
        if (locations.ContainsKey(compId))
        {
            locations = new Dictionary<long, Dictionary<long, Dictionary<string, string>>>() { { compId, locations[compId] } };
        }
    }
    string[] options = new string[] { "New quote", "Re-validate existing quote, no configuration changes", "Change configuration of existing specially quoted product" };
    string[] colors = new string[] { "ANSI 61", "RAL 1015", "RAL 1023", "RAL 1033", "RAL 2000", "RAL 2002", "RAL 2004", "RAL 3001", "RAL 3005", "RAL 3020", "RAL 5005", "RAL 5010", "RAL 5012", "RAL 5015", "RAL 5017", "RAL 5018", "RAL 6002", "RAL 6003", "RAL 6011", "RAL 7012", "RAL 7022", "RAL 7032", "RAL 7035", "RAL 9001", "RAL 9002", "RAL 9003", "RAL 9005", "RAL 9010" };
    Dictionary<string, string> colorValues = new Dictionary<string, string>() { { "ANSI 61", "Gray txt" }, { "RAL 1015", "Light Ivory txt" }, { ">RAL 1023", "Traffic Yellow txt" }, { "RAL 1033", "Dahlia Yellow txt" }, { "RAL 2000", "Yellow Orange txt" }, { "RAL 2002", "Vemilion txt" }, { "RAL 2004", " Pure Orange txt" }, { "RAL 3001", "Signal Red txt" }, { "RAL 3005", "Wine Red txt" }, { "RAL 3020", "Traffic Red txt" }, { "RAL 5005", "Signal Blue txt" }, { "RAL 5010", "Genetian Blue txt" }, { "RAL 5012", "Light Blue txt" }, { "RAL 5015", "Sky Blue txt" }, { "RAL 5017", "Traffic Blue txt" }, { "RAL 5018", "Turquoise Blue txt" }, { "RAL 6002", "Leaf Green txt" }, { "RAL 6003", "Olive Green txt" }, { "RAL 6011", " Reseda Green txt" }, { "RAL 7012", "Basalt Gray txt" }, { "RAL 7022", "Umbra Gray txt" }, { "RAL 7032", "Pebble Gray txt" }, { "RAL 7035", "Light Gray txt" }, { "RAL 9001", "Cream txt" }, { "RAL 9002", "Gray White txt" }, { "RAL 9003", "Signal White txt" }, { "RAL 9005", "Jet Black txt" }, { "RAL 9010", "Pure White txt" } };
}
