        max-height: 400px;
        overflow-y: auto;
    }

    .card-panel {
        margin: .5rem 0 1rem;
        margin-top: 0.5rem;
        background-color: #fff;
        transition: box-shadow .25s;
        border-radius: 2px;
        box-shadow: 0 8px 17px 0 rgba(0,0,0,.2),0 6px 20px 0 rgba(0,0,0,.19);
        padding: 20px;
        text-align: center;
    }

        .card-panel .centerBOMList {
            margin: 0 auto !important;
            text-align: left;
        }

        .card-panel h4 {
            width: 300px;
            text-align: left;
            margin: 0 auto;
            font-size: 110%;
            color: #e50043;
