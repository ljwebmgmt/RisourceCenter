    $(document).on('click', '.btn-add-another-accessory', function (e) {
        e.preventDefault();
        var obj = $(this);
        var tab = obj.parents('.tab.enclosure-active');
        if (!validateTab(obj.parents('.panel-body')))
            return false;
        var accordion = $('#accessory_accordion', tab);
