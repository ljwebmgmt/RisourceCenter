        if (num == '') {
            for (var j = 2; j <= 5; j++) {
                var tab = $('.tab-pane#enclosure_' + j);
                if (tab.length > 0) {
                    tab.remove();
                    $('.nav-tabs li a[href="#enclosure_' + j + '"]').parent('li').remove();
                }
            }
            return false;
        }
        for (var i = 2; i <= num; i++) {
            if ($('.tab-pane#enclosure_' + i).length > 0)
                return;
            $('.nav-tabs').append('<li role="presentation"><a href="#" data-href="#enclosure_' + i + '" role="tab">Enclosure No. ' + i + '</a></li>');
            var tab = $('.tab-pane#enclosure_1').clone();
            tab.attr('id', 'enclosure_' + i);
            tab.removeClass('active');
            var index = i - 1;
            tab.attr('data-index', index);
            $('[name]', tab).each(function () {
                var name = $(this).attr('name');
                $(this).attr('name', name.replace('enclosure[0]', 'enclosure[' + index + ']'));
            });
            $('.tab-content').append(tab);
        }
    }

    $(document).on('click', 'input[type="radio"][name]:not(.no-trigger)', function (e) {
        var obj = $(this);
        var parent = null;
        if (obj.parents('.enclosure-active').length > 0)
            parent = obj.parents('.tab.enclosure-active');
        else
            parent = obj.parents('.tab.active');
        var wrapper = $('.selection-wrapper', parent);
        if (wrapper.length == 0)
            return;
        if (obj.hasClass('selection-trigger'))
            wrapper.removeClass('hide');
        else
            wrapper.addClass('hide');
    });

    $(document).on('click', 'input[type="radio"][name].val-trigger', function (e) {
        var obj = $(this);
        var parent = null;
        if (obj.parents('.enclosure-active').length > 0)
            parent = obj.parents('.tab.enclosure-active');
        else
            parent = obj.parents('.tab.active');
        $('.selection-wrapper', parent).addClass('hide');
        var wrapper = $('.selection-wrapper[data-type="' + obj.val() + '"]', parent);
        if (wrapper.length == 0)
            return;
        wrapper.removeClass('hide');
    });

    $(document).on('change', 'select[name="product_category"]', function (e) {
        var val = $(this).val();
        if (val.toLowerCase().includes('freestanding')) {
            $('.bayed-selection-wrapper').removeClass('hide');
            $('.wallmount-hide.dont-show').removeClass('dont-show');
            $('.wallmount-show').addClass('dont-show');
        }
        else {
            $('.wallmount-hide').addClass('dont-show');
            $('.wallmount-show.dont-show').removeClass('dont-show');
            $('select[name="suited_enclosures_ie"]').val('');
            $('.bayed-selection-wrapper').addClass('hide');
            $('.industrial-selection,.it-selection').addClass('hide');
            $('.wallmount-selection').removeClass('hide');
            createEnclosures('');
        }
    });

    $(document).on('change', 'select[name].form-control', function (e) {
        var parent = $(this).parents('.form-group');
        if ($('.other-input', parent).length == 0)
            return false;
        $('.other-input', parent).addClass('hide').attr('required', false);
        if ($(this).val() == "Other")
            $('.other-input', parent).removeClass('hide').attr('required', true);
    });

    $(document).on('click', '.special-paint-radio', function (e) {
        var obj = $(this);
        var tab = obj.parents('.tab.enclosure-active');
        var parent = obj.parents('.enclosure-tab');
        if (obj.attr('data-value') == "Yes") {
            $('.color-description', tab).html('');
            var input = $('.variant-3', parent);
            input.attr('checked', true);
            input.after('<input type="hidden" data-name="' + input.attr('data-name') + '" class="variant-3-hidden" value="3" data-value="Variant 3 (Special stock color)" />');
            return;
        }
        else {
            $('.variant-3', parent).attr('checked', false);
            $('.variant-3-hidden', parent).remove();
            $('select.paint-color', tab).val('');
        }
        var enclosure = tab.parents('.tab-pane');
        $('.color-description', tab).html($('.tab.first-enclosure-section table input[type="radio"]:checked', enclosure).attr('data-color'));
    });

    $(document).on('click', '.install-accessories', function (e) {
        var obj = $(this);
        var parent = obj.parents('.enclosure-tab');
        var input = $('.variant-1', parent);
        if (obj.attr('data-value') == "Yes") {
            input.attr('checked', true);
            input.after('<input type="hidden" data-name="' + input.attr('data-name') + '" class="variant-1-hidden" value="1" data-value="Variant 1 (Assembly Only)" />');
        }
        else {
            input.attr('checked', false);
            $('.variant-1-hidden', parent).remove();
        }
    });

    $(document).on('click', '.modification-radio', function (e) {
        var obj = $(this);
        var parent = obj.parents('.enclosure-tab');
        var input = $('.variant-2', parent);
        if (obj.attr('data-value') == "Yes") {
            input.attr('checked', true);
            input.after('<input type="hidden" data-name="' + input.attr('data-name') + '" class="variant-2-hidden" value="2" data-value="Variant 2 (Assembly and/or fabrication)" />');
            triggerModifications(obj.parents('.tab.enclosure-active'));
        }
        else {
            input.attr('checked', false);
            $('.variant-2-hidden', parent).remove();
        }
    });

    $(document).on('change', '.door-dropdown', function (e) {
        var obj = $(this);
        var parent = obj.parents('.tab.enclosure-active');
        var wrapper = $('.selection-wrapper', parent);
        if (obj.val() == '' || obj.val() == 'Without' || obj.val().indexOf("Rear wall") > -1)
            wrapper.addClass('hide');
        else
            wrapper.removeClass('hide');
    });

    $(document).on('click', 'input.special-dimensions', function (e) {
        var obj = $(this);
        var parent = obj.parents('.enclosure-tab');
        if (obj.is(':checked') == 1) {
            $('.special-required', parent).attr('required', true);
            $('.products-table,.top-buttons-wrapper', parent).addClass('hide');
            $('input.enclosure-width', parent).val($('input[name="width"]', parent).val());
            $('input.enclosure-height', parent).val($('input[name="height"]', parent).val());
            $('input.enclosure-depth', parent).val($('input[name="depth"]', parent).val());
            var input = $('.variant-4', parent);
            input.attr('checked', true);
            input.after('<input type="hidden" data-name="' + input.attr('data-name') + '" class="variant-4-hidden" value="5" value="Variant 4/5 (Assembly, and/or fabrication, special stock color, and special size.)" />');
        }
        else {
            $('.special-required', parent).attr('required', false);
            $('.special-hdn-input', parent).val('');
            $('.products-table,.top-buttons-wrapper', parent).removeClass('hide');
            $('.variant-4', parent).attr('checked', false);
            $('.variant-4-hidden', parent).remove();
        }
    });

    $(document).on('change', '.special-required', function (e) {
        var obj = $(this);
        var parent = obj.parents('.enclosure-tab');
        var name = obj.attr('name');
        $('.special-hdn-input.enclosure-' + name, parent).val(obj.val());
    });

    $(document).on('click', 'input[name="default_baying_accessories"]', function (e) {
        $('.baying_hardware_wrapper').addClass('hide');
        if ($(this).val() == "0")
            $('.baying_hardware_wrapper').removeClass('hide');
    });

    $(document).on('click', 'input.accessory-guided-chk', function (e) {
        var obj = $(this);
        parent = obj.parents('.enclosure-tab');
        if (obj.val() == 1) {
            $('.guided-accessories', parent).removeClass('hide');
            $('.manual-accessories', parent).addClass('hide');
        }
        else {
            $('.guided-accessories', parent).addClass('hide');
            $('.manual-accessories', parent).removeClass('hide');
        }
    });

    $(document).on('blur', '.accessory-input', function (e) {
        var obj = $(this);
        var row = obj.parents('tr');
        var part = obj.val();
        $('td:nth-child(3)', row).text('');
        if (part == '')
            return;
        if (enclosureAccessories.hasOwnProperty(part))
            $('td:nth-child(3)', row).text(enclosureAccessories[part]);
    });

    $(document).on('blur', '.climate-input', function (e) {
        var obj = $(this);
        var row = obj.parents('tr');
        var part = obj.val();
        $('td:nth-child(3)', row).text('');
        var climateType = $('select.climate-control-type').val();
        if (part == '' || climateType == '')
            return;
        if (climateParts[climateType].hasOwnProperty(part))
            $('td:nth-child(3)', row).text(climateParts[climateType][part]);
    });

    $(document).on('click', '.btn-add-another-accessory-row', function (e) {
        e.preventDefault();
        var obj = $(this);
        var tab = obj.parents('.tab.enclosure-active');
        if (!validateTab(obj.parents('.panel-body')))
            return false;
        var row = $('.manual-accessories table tbody tr:nth-child(1)', tab).clone();
        var index = $('.manual-accessories table tbody tr', tab).length;
        $('[name]', row).each(function () {
            var name = $(this).attr('name');
            $(this).attr('name', name.replace('[manual_accessory][0]', '[manual_accessory][' + index + ']'));
            $(this).val('');
        });
        $('td:nth-child(3)', row).text('');
        $('.btn-remove-accessory-row', row).removeClass('hide');
        $('.manual-accessories table tbody', tab).append(row);
    });

    $(document).on('click', '.btn-remove-row', function (e) {
        $(this).parents('tr').remove();
    });

    $(document).on('click', '.btn-add-another-climate-row', function (e) {
        e.preventDefault();
        var obj = $(this);
        var tab = obj.parents('.tab.active');
        if (!validateTab(tab))
            return false;
        var row = $('table tbody tr:nth-child(1)', tab).clone();
        var index = $('table tbody tr', tab).length;
        $('[name]', row).each(function () {
            var name = $(this).attr('name');
            $(this).attr('name', name.replace('[climate_parts][0]', '[climate_parts][' + index + ']'));
            $(this).val('');
        });
        $('td:nth-child(3)', row).text('');
        $('.btn-remove-row', row).removeClass('hide');
        $('table tbody', tab).append(row);
    });

    $(document).on('click', '.btn-add-another-spare-row', function (e) {
        e.preventDefault();
        var obj = $(this);
        var tab = obj.parents('.tab.active');
        if (!validateTab(tab))
            return false;
        var row = $('table tbody tr:nth-child(1)', tab).clone();
        var index = $('table tbody tr', tab).length;
        $('[name]', row).each(function () {
            var name = $(this).attr('name');
            $(this).attr('name', name.replace('[spare_parts][0]', '[spare_parts][' + index + ']'));
            $(this).val('');
        });
        $('td:nth-child(3)', row).text('');
        $('.btn-remove-row', row).removeClass('hide');
        $('table tbody', tab).append(row);
    });

    $(document).on('click', '.btn-reset-climate-filters', function (e) {
        e.preventDefault();
        var obj = $(this);
        var panel = obj.parents('.panel-body');
        $('select,input[type="text"]', panel).val('');
        $('input[type="checkbox"],input[type="radio"]', panel).prop('checked', false);
        $('.apply-filters-btn', panel).trigger('click');
        return false;
    });

    $(document).on('change', '.spare-type', function (e) {
        $('.climate-control-tab,.spare-tab').addClass('dont-show');
        if ($(this).val() == 'Customized climate control product') {
            $('.climate-control-tab').removeClass('dont-show');
        }
        else {
            $('.spare-tab').removeClass('dont-show');
        }
    });
</script>

