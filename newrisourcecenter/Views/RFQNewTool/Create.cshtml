        accordion.attr('data-accessory-index', index + 1);
        $('[name]', panel).each(function () {
            var name = $(this).attr('name');
            $(this).attr('name', name.replace('[accessory][' + (index - 1) + ']', '[accessory][' + index + ']'));
            if ($(this).attr('type') == 'radio') {
                $(this).prop('checked', false);
            }
            else {
                $(this).val('');
