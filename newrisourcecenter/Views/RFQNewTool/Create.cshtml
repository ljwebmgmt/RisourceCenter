@model newrisourcecenter.Models.RFQNewViewModel

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
    int comp_type = Convert.ToInt32(Session["companyType"]);
    bool isRittalUser = comp_type == 3 || comp_type == 5;
    Dictionary<long, Dictionary<long, Dictionary<string, string>>> locations = Model.listLocations;
    if (!isRittalUser)
    {
        long compId = Convert.ToInt64(Session["companyId"]);
        if (locations.ContainsKey(compId))
        {
            locations = new Dictionary<long, Dictionary<long, Dictionary<string, string>>>() { { compId, locations[compId] } };
        }
    }
    string[] options = new string[] { "New quote", "Re-validate existing quote, no configuration changes", "Change configuration of existing specially quoted product" };
    string[] partTypeOptions = new string[] { "Bayed Configuration", "Single Enclosure" };
    string[] modOptions = new string[] { "Install Accessories", "Holes, taps, or cutouts", "Special color (stocked colors)", "Install of weld studs or weld nuts. (IF STOCKED)", "Install of self cling studs or nuts. (IF STOCKED)", "Special size, non - stock colors, special sourced parts or accessories" };
    int[] modVariants = new int[] { 1, 2, 3, 4, 4, 5 };
}
<style type="text/css">
