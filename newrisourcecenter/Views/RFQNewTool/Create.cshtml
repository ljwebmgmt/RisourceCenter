</style>
<script type="text/javascript">
    var enclosureAccessories = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model.enclosureAccessories));
    var climateParts = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model.climateParts));
    $(document).ready(function () {
        changeCompany();
        $('select[name="sap_account_num"]').trigger('change');

        $('#xpress_mod_data').change(function () {
            if ($(this).val() == "7-Day") {
                $("#enclosure_type_it option[value='server_optimized']").remove();
            } else {
                $('#enclosure_type_it').append('<option value="server_optimized">Server Optimized');
            }
        });

        $('select[name="enclosure_type_it"]').change(function () {
            if ($(this).val() == "global") {
                $('#handles_datacenter').val("Semi Cylinder");
                $('#inserts_datacenter').val("Locking");
            }
            if ($(this).val() == "server_optimized") {
                $('#handles_datacenter').val("Comfort Handle");
                $('#inserts_datacenter').val("Locking pushbutton");
            }
        }).change();

        $(function () {
            $('#updated_quote').ready(function () {
                quoteCheck($('#updated_quote').val());
            });
            $('#updated_quote').change(function () {
                quoteCheck($(this).val());
            });
        });

        function quoteCheck(val) {
            $('.quote_num').hide();
            $('.quote-text').remove();
            $('.product-wrapper').show();
            $('.submit-btn').addClass('hide');
            $('.next-button').removeClass('hide');
            $('.enclosure-tab,.product-information-tab').removeClass('dont-show');
            $('.quote_num .notes-help-text').addClass('hide');
            if (val == '')
                return;
            if (val == 'Re-validate existing quote, no configuration changes' || val == 'Change configuration of existing specially quoted product') {
                $('.quote_num').show();
                $('.enclosure-tab,.product-information-tab').addClass('dont-show');
                if (val == 'Change configuration of existing specially quoted product') {
                    $('.quote_num .field-validation-valid').after('<div class="help-text quote-text">A new quote number may be assigned</div>');
                    $('.quote_num .notes-help-text').removeClass('hide');
                }
            }
        }

        $('#plinths_ie').change(function () {
            if (this.value == 'without' || this.value == '') {
                $('.plinth_types_ie').hide();
                $('#plinths_type_ie_flex').prop('checked', '');
            } else {
                $('.plinth_types_ie').show();
                $('#plinths_type_ie_flex').prop('checked', 'true');
            }
        });

        $(function () {
            $('.color_mod_ie,.color_mod_other,.color_mod_wm,.suited_ie,.suited_enclosures_ie').hide();
            $('.color_mod_it,.suited_it,.suited_enclosures_it,.accessories_mod_it,.surfaces_mod_it').addClass('hide');
            $('input[name="part_type_it"]').change(function () {
                $('.suited_it,.suited_enclosures_it').addClass('hide');
                if ($('input[name="part_type_it"]:checked').val() == 'Bayed Configuration') {
                    $('.suited_it').removeClass('hide');
                    $('select#suited_enclosures_it').change();
                }
            });
            $('input[name="part_type_ie"]').change(function () {
                $('.suited_ie,.suited_enclosures_ie').hide();
                if ($('input[name="part_type_ie"]:checked').val() == 'Bayed Configuration') {
                    $('.suited_ie').show();
                    $('select#suited_enclosures_ie').change();
                }
            });
            $('#mods_ie').change(function () {
                var val = $(this).val().toLowerCase();
                $('.suited_ie,.color_mod_ie,.suited_enclosures_ie').hide();
                if (val.indexOf('special paint') > -1)
                    $('.color_mod_ie').show();
                else if (val == 'suited') {
                    $('.suited_ie').show();
                    $('select#suited_enclosures_ie').change();
                }
            });
            $('input[name="mods[]"]').change(function () {
                $('.color_mod_it,.accessories_mod_it,.surfaces_mod_it').addClass('hide');
                $('input[name="mods[]"]:checked').each(function () {
                    var val = $(this).val().toLowerCase();
                    if (val.indexOf('special color') > -1)
                        $('.color_mod_it').removeClass('hide');
                    else if (val.indexOf('accessories') > -1)
                        $('.accessories_mod_it').removeClass('hide');
                    var variant = $(this).attr('data-variant');
                    if (variant == 2 || variant >= 4) {
                        $('.surfaces_mod_it').removeClass('hide');
                    }
                });
            });
            $('#mods_other').change(function () {
                var val = $(this).val();
                if (val.toLowerCase().indexOf('special paint') > -1)
                    $('.color_mod_other').show();
                else
                    $('.color_mod_other').hide();
            });
            $('#mods_WM_AE_JB').change(function () {
                var val = $(this).val();
                if (val.toLowerCase().indexOf('special paint') > -1)
                    $('.color_mod_wm').show();
                else
                    $('.color_mod_wm').hide();
            });
            $('select#suited_enclosures_it').change(function () {
                $('.suited_enclosures_it').removeClass('hide');
                $('.suited_enclosures_it [data-enclosure]').hide();
                var val = $(this).val();
                for (var i = 2; i <= val; i++) {
                    $('.suited_enclosures_it [data-enclosure="' + i + '"]').show();
                }
            });
            $('select#suited_enclosures_ie').change(function () {
                $('.suited_enclosures_ie').show();
                $('.suited_enclosures_ie [data-enclosure]').hide();
                var val = $(this).val();
                for (var i = 1; i <= val; i++) {
                    $('.suited_enclosures_ie [data-enclosure="' + i + '"]').show();
                }
            });
        });

        $(function () {
            $('.ie_rating').hide();
            $('#ul_nema_other_ie').change(function () {
                if ($('#ul_nema_other_ie').val() != 'Not Rated') {
                    $('.ie_rating').show();
                } else {
                    $('.ie_rating').hide();
                }
            });
        });

        $(function () {
            $('.WM_AE_JB_rating').hide();
            $('#ul_nema_WM_AE_JB').change(function () {
                if ($('#ul_nema_WM_AE_JB').val() != 'Not Rated') {
                    $('.WM_AE_JB_rating').show();
                } else {
                    $('.WM_AE_JB_rating').hide();
                }
            });
        });

        $(function () {
            $('.other_rating').hide();
            $('#ul_nema_other').change(function () {
                if ($('#ul_nema_other').val() != 'Not Rated') {
                    $('.other_rating').show();
                } else {
                    $('.other_rating').hide();
                }
            });
        });

        addparts = function () {
            var id = $("#add_installed_parts tr:last").attr('id');
            if (!id) {
                var newid = 0;
            } else {
                var newid = parseInt(id) + 1;
            }
            $("#add_installed_parts").append('<tr id="' + newid + '" class="installed_parts_' + newid + '" >' +
                '<td>' +
                '<input class="form-control text-box single-line" id="qty_installed" name="qty_installed" type="text" value="">' +
                '<span class="field-validation-valid text-danger" data-valmsg-for="qty_installed" data-valmsg-replace="true"></span>' +
                '</td>' +
                '<td>' +
                '<input class="form-control text-box single-line" id="part_number_installed" name="part_number_installed" type="text" value="">' +
                '<span class="field-validation-valid text-danger" data-valmsg-for="part_number_installed" data-valmsg-replace="true"></span>' +
                '</td>' +
                '<td>' +
                '<input class="form-control text-box single-line" id="description_installed" name="description_installed" type="text" value="">' +
                '<span class="field-validation-valid text-danger" data-valmsg-for="description_installed" data-valmsg-replace="true"></span>' +
                '</td>' +
                '<td><div onclick="deleteinstalled(' + newid + '); return false;" class="btn btn-primary">-</div></td>' +
                '</tr>');
        }

        addshippedparts = function () {
            var id = $("#add_shipped_parts tr:last").attr('id');
            if (!id) {
                var newid = 0;
            } else {
                var newid = parseInt(id) + 1;
            }
            $("#add_shipped_parts").append('<tr id="' + newid + '" class="shipped_parts_' + newid + '" >' +
                '<td>' +
                '<input class="form-control text-box single-line" id="qty_shipped" name="qty_shipped" type="text" value="">' +
                '<span class="field-validation-valid text-danger" data-valmsg-for="qty_shipped" data-valmsg-replace="true"></span>' +
                '</td>' +
                '<td>' +
                '<input class="form-control text-box single-line" id="part_number_shipped" name="part_number_shipped" type="text" value="">' +
                '<span class="field-validation-valid text-danger" data-valmsg-for="part_number_shipped" data-valmsg-replace="true"></span>' +
                '</td>' +
                '<td>' +
                '<input class="form-control text-box single-line" id="description_shipped" name="description_shipped" type="text" value="">' +
                '<span class="field-validation-valid text-danger" data-valmsg-for="description_shipped" data-valmsg-replace="true"></span>' +
                '</td>' +
                '<td><div onclick="deleteshipped(' + newid + '); return false;" class="btn btn-primary">-</div></td>' +
                '</tr>');
        }

        deleteinstalled = function (id) {
            $(".installed_parts_" + id).remove();
        }

        deleteshipped = function (id) {
            $(".shipped_parts_" + id).remove();
        }

        $(document).on('click', '.submit-btn', function (e) {
            if ($('select[name="updated_quote"]').val() == 'Re-validate existing quote, no configuration changes') {
                if ($('#qte_num').val() == '') {
                    alert("Quote Number cannot be blank");
                    e.preventDefault();
                }
                $('[required]').attr('required', false);
            }
            else {
                if ($("#end_user_name").val() == "") {
                    alert("Complete the End User Name text box");
                    e.preventDefault();
                }
                else if ($("#end_user_location").val() == "") {
                    alert("Complete the End User Location text box");
                    e.preventDefault();
                }
                else if ($("#qte_ref").val() == "") {
                    alert("Complete the Project Name text box");
                }
                else if ($('.quote_num').is(':visible') && $('#qte_num').val() == '') {
                    alert("Quote Number / Line Item updating cannot be blank");
                    e.preventDefault();
                }
                else if ($('.product-wrapper').is(':visible') && $("#total_qty").val() == "") {
                    alert("Complete the Total Qty text box");
                    e.preventDefault();
                }
                else if ($('.product-wrapper').is(':visible') && $("#release_qty").val() == "") {
                    alert("Complete the Release Qty text box");
                    e.preventDefault();
                }
                var enclosures = [];
                var rfqVariant = 0;
                $('[name][required]').addClass('is-required').attr('required', false);
                if ($('.enclosures-wrapper').length > 0) {
                    $('.tab.enclosure-tab .tab-pane[data-index]').each(function () {
                        var index = $(this).attr('data-index');
                        var enclosure = {};
                        var namePart = 'enclosure[' + index + ']';
                        $('[name]', this).each(function () {
                            if ($(this).parents('.form-group.hide').length > 0)
                                return;
                            if (($(this).is(':radio') || $(this).is(':checkbox')) && !$(this).is(':checked'))
                                return;
                            var key = $(this).attr('name').replace(namePart, '');
                            key = key.replace('[', '');
                            key = key.replace(']', '');
                            enclosure[key] = $(this).val();
                        });
                        var variants = [];
                        $('[data-name="' + namePart + '[variants]"]:checked,input[type="hidden"][data-name="' + namePart + '[variants]"]').each(function () {
                            var val = parseInt($(this).val());
                            if (val > rfqVariant)
                                rfqVariant = val;
                            var variant = $(this).attr('data-value');
                            if ($.inArray(variant, variants) !== -1) {
                                variants.push(variant);
                            }

                        });
                        enclosure['variants'] = variants.join(', ');
                        enclosures.push(enclosure);
                    });
                    $('input[name="enclosures"]').val(JSON.stringify(enclosures));
                    $('input[name="variant"]').val(rfqVariant);
                }
                else {
                    $('input[name="climate_spare"]').val(JSON.stringify($('.spare-wrapper [name].climate-spare-input').serializeArray()));
                }
                var acc = [];
                $('[data-name="baying_accessories"]:checked').each(function () {
                    acc.push($(this).val());
                });
                $('input[name="baying_accessories"]').val(acc.join(','));
            }
        });
    });

    $(document).on('change', 'select[name="sap_account_num"]', function (e) {
        var val = $(this).val();
        $('input[name="other_sap_account_num"]').addClass('hide');
        if (val == '-1')
            $('input[name="other_sap_account_num"]').removeClass('hide');
    });

    $(document).on('change', 'select[name="sold_to_party"]', function (e) {
        changeCompany()
    });

    function changeCompany() {
        var select = $('select[name="sold_to_party"]');
        $('select[name="location"] option:not(.first)').addClass('hide');
        $('select[name="location"],input[name="sap_account_num"]').val('');
        if (select.val() == '')
            return;
        var compId = $('option:selected', select).attr('data-id');
        $('select[name="location"] option[data-company="' + compId + '"]').removeClass('hide');
    }

    $(document).on('change', 'select[name="location"]', function (e) {
        $('input[name="sap_account_num"]').val('');
        if ($(this).val() == '')
            return;
        $('input[name="sap_account_num"]').val($('option:selected', this).attr('data-sap'));
    });

    $(document).on('change', '.trigger-filter', function (e) {
        var parent = $(this).parents('.tab.enclosure-active');
        if ($('.special-dimensions', parent).is(':checked'))
            return false;
        $('.btn-apply-filters', parent).trigger('click');
    });

    $(document).on('click', '[name="product_industry"]', function (e) {
        var val = $(this).val().toLowerCase();
        $('.' + val + '-selection:not(.double-door)').removeClass('hide');
        $('.' + (val == 'industrial' ? 'it' : 'industrial') + '-selection').addClass('hide');
        if (val == 'it')
            $('.it-hide:not(.dont-show)').addClass('dont-show');
        else
            $('.it-hide').removeClass('dont-show');
    });

    var rackUnits = { '47': 2200, '48': 2200, '42': 2000, '24': 1200 };
    $(document).on('click', '.btn-apply-filters', function (e) {
        e.preventDefault();
        var obj = $(this);
        var parent = obj.parents('.tab.enclosure-active');
        if (populateFilters) {
            $('[name]:not(.disabled):not([type="radio"])', parent).val('');
        }
        var data = $('[name]', parent).serializeArray();
        data.push({ name: 'product_type', value: $('select[name="product_category"]').val() });
        data.push({ name: 'industry', value: ($('select[name="product_category"]').val() == 'Freestanding' ? $('[name="product_industry"]:checked').val() : "") });
        data.push({ name: 'return_filters', value: true });
        $.ajax({
            url: '/RFQNewTool/GetProducts',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                obj.html('Loading....');
            },
            success: function (res) {
                createProductsTable(res.products, parent);
                var materials = [];
                $.each(['material', 'family', 'rating', 'enclosure_type'], function (i, field) {
                    if (!res.hasOwnProperty(field) || res[field].length == 0)
                        return;
                    var inputVal = $('select[name="' + field + '"]').val();
                    if (inputVal == '') {
                        var html = '<option value="">Select Option</option>';
                        $.each(res[field], function (j, item) {
                            var optVal = item;
                            if (field == 'material') {
                                if (item.indexOf('-') > -1) {
                                    optVal = item.split('-')[0].trim();
                                    if (materials.indexOf(optVal) > -1) {
                                        return;
                                    }
                                }
                                materials.push(optVal);
                            }
                            html += '<option value="' + optVal + '">' + optVal + '</option>';
                        });
                        $('select[name="' + field + '"]', parent).html(html);
                    }
                });
                populateFilters = false;
            },
            complete: function () {
                obj.html('Apply Filters');
            }
        });
        return false;
    });

    $(document).on('click', '.btn-apply-accessory-filters', function (e) {
        e.preventDefault();
        var obj = $(this);
        var parent = obj.parents('.tab.enclosure-active');
        var wrapper = obj.parents('.accessory-filters');
        var data = [];
        $('[data-name]', wrapper).each(function () {
            data.push({ name: $(this).attr('data-name'), value: $(this).val() });
        });
        data.push({ name: 'product_type', value: $('select[name="product_category"]').val() });
        var enclosure = parent.parents('.tab-pane');
        data.push({ name: 'product_family', value: $('.tab.first-enclosure-section table input[type="radio"]:checked', enclosure).attr('data-family') });
        $.ajax({
            url: '/RFQNewTool/GetAccessories',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                obj.html('Loading....');
            },
            success: function (res) {
                var panel = $('.accessory-panel .panel-collapse.in:visible', parent);
                if (!res.accessories || res['accessories'].length == 0)
                    return;
                var table = $('.accessory-table', panel);
                var html = createAccessoriesTable(res.accessories, table.attr('data-name'));
                $('tbody', table).html(html);
                table.removeClass('hide');
            },
            complete: function () {
                obj.html('Apply Filters');
            }
        });
        return false;
    });

    function createAccessoriesTable(products, inputName) {
        var html = '';
        if (products.length > 0) {
            $.each(products, function (i, prod) {
                html += '<tr><td class="text-center"><input type="radio" value="' + prod.part_number + '" name="' + inputName + '" class="no-trigger" /><td>' + prod.part_number + '</td><td>' + prod.product_type + '</td><td>' + prod.family + '</td><td>' + prod.group_1 + '</td><td>' + prod.group_2 + '</td><td>' + prod.description_1 + '</td><td>' + prod.description_2 + '</td><td>' + prod.description_3 + '</td><td>' + (prod.color == 'null' ? '' : prod.color) + '</td><td>' + prod.material + '</td></tr> ';
            });
        }
        else {
            html = '<tr><td colspan="11" class="text-center">No accessories found</td></tr>';
        }
        return html;
    }

    $(document).on('change', 'select[data-name="group_1"]', function (e) {
        var wrapper = $(this).parents('.accessory-filters');
        var group = $(this).val();
        var select = $('select[data-name="group_2"]', wrapper);
        if (group == '') {
            select.html('<option value="">Select Option</option>').val('');
            return false;
        }
        var data = [];
        data.push({ name: 'group_1', value: group });
        data.push({ name: 'product_type', value: $('select[name="product_category"]').val() });
        var enclosure = wrapper.parents('.tab-pane');
        data.push({ name: 'product_family', value: $('.tab.first-enclosure-section table input[type="radio"]:checked', enclosure).attr('data-family') });
        $.ajax({
            url: '/RFQNewTool/GetAccessoryGroups',
            type: 'POST',
            data: data,
            dataType: 'json',
            beforeSend: function () {
                select.addClass('hide');
                select.after('<div class="loader">Loading....</div>');
            },
            success: function (res) {
                if (!res.groups || res['groups'].length == 0)
                    return;
                var options = '<option value="">Select Option</option>';
                $.each(res.groups, function(i, group) {
