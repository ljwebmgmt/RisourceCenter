<style type="text/css">
    input.rfq_text_input {
        width: 250px;
    }

    div.box {
        display: none;
    }

    div.ts8_it {
        background: #DEF2E1;
        padding: 15px 10px 60px 30px;
        margin: 25px;
    }

    div.ts8_ie {
        background: #E5E2AE;
        padding: 15px 10px 60px 30px;
        margin: 25px;
    }

    div.WM_AE_JB_box {
        background-color: #BBDDF7;
        padding: 15px 10px 60px 30px;
        margin: 25px;
    }

    div.other_box {
        background-color: #D7D5DD;
        padding: 15px 10px 60px 30px;
        margin: 25px;
    }

    input.rfq_text_input_table {
        margin: 2px;
    }

    div.quote_num {
        display: none;
    }

    .button-success,
    .button-error,
    .button-warning,
    .button-secondary {
        color: white;
        border-radius: 4px;
        text-shadow: 0 1px 1px rgba(0, 0, 0, 0.2);
        background-color: black;
    }

    .button-error {
        background: rgb(202, 60, 60); /* this is a maroon */
    }

    .table {
        width: 80%;
        margin: 40px auto 40px auto;
    }

    #additional_information {
        padding: 15px 10px 60px 30px;
        margin: 25px;
        margin-top: -30px;
    }
</style>
<script type="text/javascript">
    $(document).ready(function(){

        $("#product_category").change(function () {
            $("select option:selected").each(function () {
                var val = $(this).attr("value");
                if (val == "ts8-it") {
                    $(".box").hide();
                    $(".ts8_it").show();
                    $("#additional_information").show().css("background", "#DEF2E1");
                }
                else if (val == "ts8-ie") {
                    $(".box").hide();
                    $(".ts8_ie").show();
                    $("#additional_information").show().css("background", "#E5E2AE");
                }
                else if (val == "WM_AE_JB") {
                    $(".box").hide();
                    $(".WM_AE_JB_box").show();
                    $("#additional_information").show().css("background", "#BBDDF7");
                }
                else if (val == "Other") {
                    $(".box").hide();
                    $(".other_box").show();
                    $("#additional_information").show().css("background", " #D7D5DD");
                }
                else if (val == "select") {
                    $(".box").hide();
                    $("#additional_information").hide();
                }
            });
        }).change();

		$('#xpress_mod_data').change(function() {

		 if($(this).val() =="7-Day") {
					$("#enclosure_type_it option[value='server_optimized']").remove();
				} else {
					$('#enclosure_type_it').append('<option value="server_optimized">Server Optimized');
				}

		});

        $(document).ready(function() {
    	    $('select[name="enclosure_type_it"]').change(function() {
    		    if ($(this).val() == "global") {
    			    $('#handles_datacenter').val("Semi Cylinder");
    			    $('#inserts_datacenter').val("Locking");
    		    }
    		    if ($(this).val() == "server_optimized") {
    			    $('#handles_datacenter').val("Comfort Handle");
    			    $('#inserts_datacenter').val("Locking pushbutton");
    		    }
    	    }).change();
        });

        $(function () {
            $('#updated_quote').ready(function () {
                quoteCheck($('#updated_quote').val());
            });
            $('#updated_quote').change(function () {
                quoteCheck($(this).val());
            });
        });

        function quoteCheck(val) {
            $('.quote_num').hide();
            $('.quote-text').remove();
            if (val == '')
                return;
            if (val == 'Re-validate expiration date of existing quote' || val == 'Change configuration of existing specially quoted product' || val == 'Requote of an existing quote number') {
                $('.quote_num').show();
                if (val == 'Re-validate expiration date of existing quote') {
                    $(this).after('<span class="help-text quote-text">A new quote number may be assigned</span>')
                }
            }
        }
        $(function() {
            $('.28_color_mod_ie').hide();
            $('#special_paint_ie').ready(function(){
                if($('#special_paint_ie').val() == 'standard') {
                    $('.28_color_mod_ie').hide();
                } else {
                    $('.28_color_mod_ie').show();
                }
            });
            $('#special_paint_ie').change(function () {
                if ($('#special_paint_ie').val() == 'standard') {
                    $('.28_color_mod_ie').hide();
                } else {
                    $('.28_color_mod_ie').show();
                }
            });
        });

        $(function() {
            $('.28_color_mod_wm').hide();
            $('#special_paint_wm').ready(function(){
                if($('#special_paint_wm').val() == 'standard') {
                    $('.28_color_mod_wm').hide();
                } else {
                    $('.28_color_mod_wm').show();
                }
            });
            $('#special_paint_wm').change(function () {
                if ($('#special_paint_wm').val() == 'standard') {
                    $('.28_color_mod_wm').hide();
                } else {
                    $('.28_color_mod_wm').show();
                }
            });
        });

        $(function() {
            $('.ie_rating').hide();
            $('#ul_nema_other_ie').ready(function(){
                if($('#ul_nema_other_ie').val() != 'Not Rated') {
                    $('.ie_rating').show();
                } else {
                    $('.ie_rating').hide();
                }
            });
            $('#ul_nema_other_ie').change(function () {
                if ($('#ul_nema_other_ie').val() != 'Not Rated') {
                    $('.ie_rating').show();
                } else {
                    $('.ie_rating').hide();
                }
            });
        });

        $(function() {
            $('.WM_AE_JB_rating').hide();
            $('#ul_nema_WM_AE_JB').ready(function(){
                if($('#ul_nema_WM_AE_JB').val() != 'Not Rated') {
                    $('.WM_AE_JB_rating').show();
                } else {
                    $('.WM_AE_JB_rating').hide();
                }
            });
            $('#ul_nema_WM_AE_JB').change(function () {
                if ($('#ul_nema_WM_AE_JB').val() != 'Not Rated') {
                    $('.WM_AE_JB_rating').show();
                } else {
                    $('.WM_AE_JB_rating').hide();
                }
            });
        });

        $(function() {
            $('.other_rating').hide();
            $('#ul_nema_other').ready(function () {
                if ($('#ul_nema_other').val() != 'Not Rated') {
                    $('.other_rating').show();
                } else {
                    $('.other_rating').hide();
                }
            });
            $('#ul_nema_other').change(function(){
                if($('#ul_nema_other').val() != 'Not Rated') {
                    $('.other_rating').show();
                } else {
                    $('.other_rating').hide();
                }
            });
        });
    });
</script>
@if (!string.IsNullOrEmpty(@ViewBag.error))
{
    <h2 style="color:red;">@ViewBag.error</h2>
}

@if (!string.IsNullOrEmpty(Request.QueryString["success"]))
