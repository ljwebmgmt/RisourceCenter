@if (Model.RFQExt != null)
{
    <div style="width:100%;border: solid 1px #BBDDF7;height:225px;overflow:auto;">
        <h3>&nbsp;<u>Additional Enclosures</u></h3>
        <div style="width:19%;border: solid 1px #BBDDF7;padding:20px;margin: 0px 0px 5px 10px;display:inline-block;background:#a4f19e;">
            <strong style="color:red;">RFQ Form</strong>
            <br /><br />
            <span><strong>Product Category :</strong> @ViewBag.product_category</span><br />
            <span><strong>Total Quantity :</strong> @ViewBag.total_qty</span><br />
            <br />
            <span>
                @Html.ActionLink("View", "Details", new { id = @ViewBag.original_id, n1_name = @Request.QueryString["n1_name"], n2_name = @Request.QueryString["n2_name"], ext_form = "no" }, new { @class = "btn btn-info" })
            </span>
            <br />
        </div>
        @foreach (var item in Model.RFQExt)
        {
            <div style="width:19%;border: solid 1px #BBDDF7;padding:20px;margin: 0px 0px 5px 10px;display:inline-block;">
                <span><strong>Product Category :</strong> @item.product_categories</span><br />
                <span><strong>Total Quantity :</strong> @item.total_quantity</span><br />
                <br />
                <span>
                    @Html.ActionLink("View", "DetailsExt", new { id = item.rfqidExt, form_id = item.rfqid, n1_name = @Request.QueryString["n1_name"], n2_name = @Request.QueryString["n2_name"], ext_form = "yes" }, new { @class = "btn btn-info" })
                </span>&nbsp;&nbsp;
                <br />
                <br />
            </div>
        }
    </div>
}

