@model newrisourcecenter.Models.partnerStockCheckViewModel

@{
    Layout = null;
}

<tr>
    <td>
        @Model.ps_account
    </td>
    <td>
        @Model.company_name
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id=Model.ps_ID, n1_name = @Request.QueryString["n1_name"], n2_name = @Request.QueryString["n2_name"], msg = @Request.QueryString["msg"], user_ID = Model.usr_user, compid = Request.QueryString["dcompid"] }, new { @class = "btn btn-info" }) 
        @Html.ActionLink("Delete", "Delete", new { id=Model.ps_ID, n1_name = @Request.QueryString["n1_name"], n2_name = @Request.QueryString["n2_name"], msg = @Request.QueryString["msg"], user_ID = Model.usr_user, compid = Request.QueryString["dcompid"] }, htmlAttributes: new { id = "delete", onclick = "deletef(this.href);return false", @class = "btn btn-info" })
    </td>
</tr>
