@model IEnumerable<newrisourcecenter.Models.Nav3ViewModel>
@foreach (var item in Model)
{

    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.n3order)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.n3_nameLong)
            @if (Request.QueryString["translate"] != null)
            {
                @Html.ActionLink("T", "Create", "Localization", new { parent_id = @item.n3ID, tableName = "nav3", columnName = "n3_nameLong", message = @item.n3_nameLong, n1_name = "Translate Page Content", msg = Request.QueryString["n1_name"] }, htmlAttributes: new { @id = "translate", @class = "glyphicon glyphicon-edit" })
            }
        </td>
        <td>
            @Html.Raw(item.n3_descLong)
            @if (Request.QueryString["translate"] != null)
            {
                @Html.ActionLink("T", "Create", "Localization", new { parent_id = @item.n3ID, tableName = "nav3", columnName = "n3_descLong", message = @item.n3_descLong, n1_name = "Translate Page Content", msg = Request.QueryString["n1_name"] }, htmlAttributes: new { @id = "translate", @class = "glyphicon glyphicon-edit" })
            }
        </td>
        @if (Request.QueryString["n2Id"] == null)
        {
            <td>
                @Html.ActionLink(@Model.LastOrDefault().nav3labels.FirstOrDefault().edit_label[0], "Edit", new { id = item.n3ID, n1_name = Request.QueryString["n1_name"], msg = item.n3_nameLong }, new { @class = "btn btn-info" })
                @if (Request.QueryString["translate"] != null)
                {
                    @Html.ActionLink("T", "Create", "Labels", new { labelName = @Model.LastOrDefault().nav3labels.FirstOrDefault().edit_label[1], controllerName = "Nav3ViewModel", pageName = "Index", n1_name = "Translate Page Labels", msg = Request.QueryString["n1_name"] }, htmlAttributes: new { @id = "translate", @class = "glyphicon glyphicon-edit" })
                }
                @Html.ActionLink(@Model.LastOrDefault().nav3labels.FirstOrDefault().delete_label[0], "Delete", new { id = item.n3ID, n2Id = Request.QueryString["n2Id"], n1_name = Request.QueryString["n1_name"], msg = item.n3_nameLong }, htmlAttributes: new { id = "delete", onclick = "deletef(this.href);return false", @class = "btn btn-info" })
                @if (Request.QueryString["translate"] != null)
                {
                    @Html.ActionLink("T", "Create", "Labels", new { labelName = @Model.LastOrDefault().nav3labels.FirstOrDefault().delete_label[1], controllerName = "Nav3ViewModel", pageName = "Index", n1_name = "Translate Page Labels", msg = Request.QueryString["n1_name"] }, htmlAttributes: new { @id = "translate", @class = "glyphicon glyphicon-edit" })
                }
            </td>
        }
        else
        {
            <td>
                @Html.ActionLink(@Model.LastOrDefault().nav3labels.FirstOrDefault().edit_label[0], "Edit", new { id = item.n3ID, n1_name = Request.QueryString["n1_name"], n2_name = Request.QueryString["n2_name"], msg = item.n3_nameLong }, new { @class = "btn btn-info" })
                @if (Request.QueryString["translate"] != null)
                {
                    @Html.ActionLink("T", "Create", "Labels", new { labelName = @Model.LastOrDefault().nav3labels.FirstOrDefault().edit_label[1], controllerName = "Nav3ViewModel", pageName = "Index", n1_name = "Translate Page Labels", msg = Request.QueryString["n1_name"] }, htmlAttributes: new { @id = "translate", @class = "glyphicon glyphicon-edit" })
                }
                @Html.ActionLink(@Model.LastOrDefault().nav3labels.FirstOrDefault().delete_label[0], "Delete", new { id = item.n3ID, n2Id = Request.QueryString["n2Id"], n1_name = Request.QueryString["n1_name"], n2_name = Request.QueryString["n2_name"], msg = item.n3_nameLong }, htmlAttributes: new { id = "delete", onclick = "deletef(this.href);return false", @class = "btn btn-info" })
                @if (Request.QueryString["translate"] != null)
                {
                    @Html.ActionLink("T", "Create", "Labels", new { labelName = @Model.LastOrDefault().nav3labels.FirstOrDefault().delete_label[1], controllerName = "Nav3ViewModel", pageName = "Index", n1_name = "Translate Page Labels", msg = Request.QueryString["n1_name"] }, htmlAttributes: new { @id = "translate", @class = "glyphicon glyphicon-edit" })
                }
            </td>
        }
    </tr>

}
